 50) {\n      this.setState({\n        isVisible: true,\n      });\n    } else {\n      this.setState({\n        isVisible: false,\n      });\n    }\n  }\n\n  scrollToTop() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth',\n    });\n  }\n\n  render() {\n    return (\n      <div className={styles.scrollTop}>\n        {this.state.isVisible && (\n          <div onClick={() => this.scrollToTop()}>\n            <button className={styles.scrollTopButton}>\n              <i className={styles.upArrow}></i>\n            </button>\n          </div>\n        )}\n      </div>\n    );\n  }\n}\n","import React from 'react';\n\nimport { NavDropdown, Card, Row, Col, Container } from 'react-bootstrap';\nimport logo from '../../Assets/amdocs.png';\nimport styles from './Opportunity.module.css';\n\nexport function OpportunityCard(props) {\n  const { item } = props;\n\n  return (\n\n       \n      <Card className={styles.Card}>\n        <Container>\n          <Row>\n            <Col>\n            <Card.Img\n           \n           src={logo}\n           // src = {item.imgURL}\n           alt={item.title}\n           className={styles.CardImage}\n         />\n            </Col>\n            <Col>\n            <Card.Title className={styles.Title}>{item.title}</Card.Title>\n            </Col>\n          </Row>\n        </Container>\n     \n         \n          \n          <Card.Body>\n         \n            <Card.Text>\n          <p className={styles.Description}>{item.description}</p>\n       \n            <Container>\n              <Row>\n                <Col>\n                <h5 className={styles.Heading} >\n                  Date :\n               \n                <span className={styles.Value}>{item.date} </span></h5>\n                </Col>\n                <Col>\n                    <h5 className={styles.Heading} >\n                  Location :\n                  <span className={styles.Value}>{item.location}</span>\n                </h5>\n               \n      